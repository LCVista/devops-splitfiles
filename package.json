{
  "name": "pr-deployer",
  "packageManager": "yarn@3.2.2",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "format": "prettier --write --ignore-path 'dist/**' '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "yarn jest --coverage --silent --no-watchman",
    "test-with-debug": "yarn jest --coverage --silent --no-colors --no-watchman --detectOpenHandles --forceExit",
    "prebuild": "rm -rf dist",
    "build": "esbuild src/*.ts --bundle --minify --platform=node --target=es2020 --outdir=dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LCVista/devops-pr-deployer.git"
  },
  "keywords": [],
  "author": "LCvista",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LCVista/devops-pr-deployer/issues"
  },
  "homepage": "https://github.com/LCVista/devops-pr-deployer#readme",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@octokit/core": "^3.5.1",
    "fast-glob": "^3.2.11"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "esbuild": "^0.14.51",
    "eslint": "^8.7.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.2"
  }
}
